{
  "files.exclude": {
    "**/*.dSYM/": true
  },
  "files.associations": {
    "*.x": "lex",
    "*.y": "yacc"
  },
  "[yacc]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[lex]": {
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[c]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "C_Cpp.default.includePath": ["./include"],
  "C_Cpp.default.configurationProvider": "ms-vscode.makefile-tools"
}
